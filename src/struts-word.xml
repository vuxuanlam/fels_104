<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <!-- Configure NameSpace for action web -->
    <package name="Word" namespace="/word" extends="default-web">
        <action name="listWord" class="springWordAction" method="listWord">
            <result name="success">Word.All</result>
            <result name="error">HomePage</result>
            <result name="none">HomePage</result>
        </action>
    </package>
</struts>